{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    ":label(dependencies)",
    ":semanticCommitsDisabled"
  ],
  ignorePresets: [
    "mergeConfidence:all-badges"
  ],
  prBodyColumns: [
    "Package",
    "Change",
    "Diffend"
  ],
  prBodyDefinitions: {
    Diffend: ""
  },
  packageRules: [
    {
      matchDatasources: [
        "go",
        "maven",
        "npm",
        "nuget",
        "packagist",
        "pypi",
        "rubygems",
      ],
      matchUpdateTypes: [
        "patch",
        "minor",
        "major"
      ],
      prBodyColumns: [
        "Package",
        "Change",
        "Age",
        "Adoption",
        "Passing",
        "Confidence",
        "Diffend"
      ]
    },
    {
      matchManagers: [
        "bundler"
      ],
      prBodyDefinitions: {
        Diffend: "https://my.diffend.io/gems/{{depName}}/{{currentVersion}}/{{newVersion}}"
      }
    },
    {
      matchManagers: [
        "npm"
      ],
      prBodyDefinitions: {
        Diffend: "https://my.diffend.io/npm/{{depName}}/{{currentVersion}}/{{newVersion}}"
      }
    }
  ]
}
